#include <stdio.h>

int main(void)
{
    double length;
    double height;
    double area;

    printf("Enter triangle's length : ");
    scanf("%f, &length");
    printf("Enter triangle's length : ");
    scanf("%f, &length");
    area = length * height / 2.0 ;
    printf("triangle's area is : %f\n, area");
        return 0;
}